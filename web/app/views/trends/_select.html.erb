<div class="selector-wrap">
  <p class="selector">
    <% dates=[['最近24小时(Last 24 hours)','last24hours'],['今天(Today)','today'],['昨天(Yesterday)','yesterday'],['本周(This Week)','thisweek'],['最近7天(Last 7 days)','last7days'],['本月(This Month)','thismonth'],['最近30天(Last 30 days)','last30days']] %>
    <% dates.each do |d| %>
      <%= link_to d[0] ,{:controller=>:trends,:action=>:index,:date=>d[1],:object=>@object,:instance=>@instance,:serviceid=>@serviceid}, :class => (d[1]==@date ? "selected" : nil) %>
    <% end %>
  </p>
  <p class="selector">
    <% objects={'主机'=>'1','应用'=>'2','网站'=>'3','网络'=>'4'} %>
    <% objects.each do |k,v|%>
      <%= link_to k, {:controller=>:trends,:action=>:index,:object=>v,:date=>@date,:instance=>@instance,:serviceid=>@serviceid}, :class => (v==@object ? "selected" : nil) %>
    <% end %>
  </p>
  <p class="selector">
    <% if !@instances.nil? %>
      <%  @instances.each do |n| %>
        <%= link_to n.name, {:controller=>:trends,:action=>:index,:instance=>n.id,:object=>@object,:date=>@date,:serviceid=>@serviceid}, :class =>(n.id.to_s==@instance ? "selected" : nil) %>
      <% end %>
    <%end%>
  </p>
  <p class="selector">
    <% if !@services.nil? %>
      <%  @services.each do |n| %>
        <%= link_to n.name,{:controller=>:trends,:action=>:index,:serviceid=>n.id,:instance=>@instance,:object=>@object,:date=>@date},:class =>(n.id.to_s == @serviceid ? "selected" : nil) %>
      <% end %>
    <%end%>
  </p>
</div>
