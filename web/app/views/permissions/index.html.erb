<h1>Listing permissions</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Tenant</th>
    <th>Order seq</th>
    <th>Module name</th>
    <th>Name</th>
    <th>Description</th>
    <th>Controller name</th>
    <th>Action name</th>
    <th>Default permission</th>
    <th>Method</th>
    <th>Module text</th>
    <th>Format</th>
    <th>Created at</th>
    <th>Updated at</th>
  </tr>

<% @permissions.each do |permission| %>
  <tr>
    <td><%=h permission.id %></td>
    <td><%=h permission.tenant_id %></td>
    <td><%=h permission.order_seq %></td>
    <td><%=h permission.module_name %></td>
    <td><%=h permission.name %></td>
    <td><%=h permission.description %></td>
    <td><%=h permission.controller_name %></td>
    <td><%=h permission.action_name %></td>
    <td><%=h permission.default_permission %></td>
    <td><%=h permission.method %></td>
    <td><%=h permission.module_text %></td>
    <td><%=h permission.format %></td>
    <td><%=h permission.created_at %></td>
    <td><%=h permission.updated_at %></td>
    <td><%= link_to 'Show', permission %></td>
    <td><%= link_to 'Edit', edit_permission_path(permission) %></td>
    <td><%= link_to 'Destroy', permission, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New permission', new_permission_path %>