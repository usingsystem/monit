<div class="datagrid">
  <h5>DNS响应时间TopN</h5>
  <table cellspacing="0">
    <thead>
      <tr>
        <%=th_tag "网站"%>
        <%=th_tag "DNS响应时间TopN"%>
      </tr>
    </thead>
    <% if @dns %>
      <tbody>
        <% if @dns.size > 0 %>
          <% @dns.each do |type| %>
            <tr>
              <%=td_tag type["name"]%>
              <%=td_tag ('%.2f' %(type["time"]))%>
            </tr>
          <% end %>
        <% end %>
      </tbody>
    <%end%>
  </table>
</div>
