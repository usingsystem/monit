<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
"http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="<%=I18n.locale%>" lang="<%=I18n.locale%>">
  <%content_for :head do %>
    <%=stylesheet_link_tag 'entry',:media=>'all' %>
  <% end %>
  <%=render :partial=>'layouts/head'%>
  <body>
    <div id="wrapper">
      <!--header-->
      <div id="header" class="clearfix">
        <h2 id="logo">
          <%=link_to (image_tag "index_logo.png", :alt => t('production_name')) + "<em>For admin \!</em>", "/" %>
        </h2>
      </div>
      <!--header end-->
      <div class="wrap">
        <div class="entry">
          <div class="entry-t">
            <div class="entry-b clearfix">
              <%=yield%>
            </div>
          </div>
        </div>
      </div>
      <%=render :partial=>'layouts/footer'%>
    </div>
    <%=render :partial=>'layouts/analytics'%>
  </body>
</html>

